  "html": "{{ letter_head }}\n\n{%- if trigger_print -%}\n<script>\nwindow.print();\n\n// close the window after print\n// NOTE: doesn't close if print is cancelled in Chrome\nwindow.onfocus = function() {\nwindow.close();\n}\n</script>\n{%- endif -%}\n\n<div class=\"row\">\n    <div class=\"col-xs-6\"></div>\n    <div class=\"col-xs-6\" align=\"right\">\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <h4 style=\"margin:0;cellpadding:0\" align=\"right\"><b>QUOTE</b></h4>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n            <table width=50% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n              <th>Date</th>\n              <th>Quote#</th>\n              <tr>\n                  <td>{{ doc.get_formatted(\"transaction_date\",doc) or \"\" }}</td>\n                  <td>{{ doc.name or \"\" }} </td>\n              </tr>\n           </table>\n</div>\n        </div>\n      </div>\n    <div class=\"col-xs-6\"></div>\n</div>\n<hr>\n<div class=\"row\">\n      <div class=\"col-xs-6\">\n          <!-- Bill To details -->\n          <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n              <th><b>Name / Address</b></th>\n              <tr>\n                  <td style=\"height: 130px;display: block;\">\n                      {{ doc.customer_name or \"\" }}\n                  <br>{{ doc.address_display or \"\" }}\n                  </td>\n              </tr>\n          </table>\n      </div>\n      <div class=\"col-xs-6\">\n          <!-- Ship To Details  -->\n          <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n              <th><b>Ship To</b></th>\n              <tr>\n                  <td style=\"height: 130px;display: block;\">\n                      {{ doc.contact_display or \"\" }}\n                  <br>{{ doc.shipping_address or \"\" }}\n                  </td>\n              </tr>\n          </table>\n      </div>\n  </div>\n  <br>\n  <br>\n  <br>\n  <div class=\"row\">\n      <div class=\"col-xs-6\">\n      </div>\n      <div class=\"col-xs-6\">\n          <!-- Terms Table  -->\n          <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n              <th>Term</th>\n              <th>Rep</th>\n              <tr>\n                  <td>{{ doc.tc_name or \"\" }}</td>\n                  <td>{{ doc.employee or \"\"}} {% if doc.employee %} - {{ doc.employee_name or \"\"}}{% endif %}</td>\n              </tr>\n          </table>\n      </div>\n  </div>\n  <div class=\"row\">\n      <div class=\"col-xs-12\">\n          <!-- Item Table -->\n          <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n              <th>Item</th>\n              <th>Rate</th>\n              <th>Description</th>\n              <th>Qty</th>\n              <th>Total</th>\n              {%- for row in doc.items -%}\n              <tr>\n                  <td>{{ row.item_name or \"\" }}</td>\n                  <td align=\"right\">{{ row.rate or \"\" }}</td>\n                  <td>{{ row.description or \"\" }}</td>\n                  <td align=\"right\">{{ row.qty or \"\" }}</td>\n                  <td align=\"right\">{{ row.amount or \"\" }}</td>\n              </tr>\n              {%- endfor -%}\n              <tr>\n                  <td colspan=\"3\"></td>\n                  <td colspan=\"2\" align=\"center\">\n                      <b>Total :</b> {{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") or \"\" }} {{doc.grand_total or \"\"}}\n                  </td>\n              </tr>\n          </table>\n      </div>\n  </div>", 
