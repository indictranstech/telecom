  "html": "{{ letter_head }}\n\n{%- if trigger_print -%}\n<script>\nwindow.print();\n\n// close the window after print\n// NOTE: doesn't close if print is cancelled in Chrome\nwindow.onfocus = function() {\nwindow.close();\n}\n</script>\n{%- endif -%}\n\n<div class=\"row\">\n    <div class=\"col-xs-12\">\n        <div class=\"row\">\n            <div class=\"col-xs-6\"></div>\n            <div class=\"col-xs-6\" align=\"right\">\n                <div class=\"row\">\n                    <div class=\"col-xs-12\">\n                        <h4 style=\"margin:0;cellpadding:0\" align=\"right\"><b>SUPPORT QUEUE</b></h4>\n                    </div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-12\">\n<table width=35% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n            <th>Issue#</th>\n              <tr>\n                 <td>{{ doc.name or \"\" }} </td>\n              </tr>\n           </table>\n</div>\n                </div>\n            </div>\n            <div class=\"col-xs-4\"></div>\n        </div>\n        <!-- <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <div class=\"row\">\n                    <div class=\"col-xs-6\" align=\"right\"><b>Issue#</b></div>\n                    <div class=\"col-xs-5\">{{ doc.name or \"\"}}</div>\n                </div>\n            </div>\n        </div> -->\n        <hr>\n        <!-- <div class=\"row\">\n            <div class=\"col-xs-6\"></div>\n            <div class=\"col-xs-6\">\n                <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n                    <tr align=\"center\">\n                       <td><b>Issue#</b></td>\n                    </tr>\n                    <tr align=\"center\">\n                       <td>{{ doc.name or \"\" }}</td>\n                    </tr>\n                </table>\n            </div>\n        </div> -->\n        <div class=\"row\">\n            <div class=\"col-xs-6\">\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Customer Name</b></div>\n                    <div class=\"col-xs-7\">{{doc.customer_name or \"\"}}</div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Location ID</b></div>\n                    <div class=\"col-xs-7\">{{doc.location_id or \"\"}}</div>\n                </div>\n                {% set location_name = frappe.db.get_value(\"Address\",doc.location_id,\"location_name\") %}\n                {% if location_name %}\n                    <div class=\"row\">\n                        <div class=\"col-xs-5\" align=\"right\"><b>Location Name</b></div>\n                        <div class=\"col-xs-7\">{{location_name or \"\"}}</div>\n                    </div>\n                {% endif %}\n                {% if doc.location_id %}\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Address</b></div>\n                    {% if doc.address_display %}\n                    <div class=\"col-xs-7\">{% set ad = doc.address_display.split(\"<br>\")[2:-1] %} {{'<br>'.join(ad) }}</div>\n                    {% endif %}\n                </div>\n                {% endif %}\n            </div>\n            <div class=\"col-xs-6\">\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Status</b></div>\n                    <div class=\"col-xs-7\">{{doc.status or \"\"}}</div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Contact</b></div>\n                    <div class=\"col-xs-7\">\n{% if doc.contact -%}{{doc.contact.split(\"-\")[0]  or \"\"}}{% endif %}</div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Phone</b></div>\n                    <div class=\"col-xs-7\">{{doc.phone or \"\"}}</div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Raised By</b></div>\n                    <div class=\"col-xs-7\">{{doc.raised_by or \"\"}}</div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Assigned To</b></div>\n                    <div class=\"col-xs-7\">{{frappe.db.get_value(\"ToDo\",{\"reference_type\":\"Issue\",\"reference_name\":doc.name, \"status\":\"Open\"}, \"owner\") or \"\"}}</div>\n                </div>\n            </div>\n        </div>\n        <hr>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <div class=\"row\">\n                    <div class=\"col-xs-2\" align=\"right\"><b>Subject</b></div>\n                    <div class=\"col-xs-10\">{{ doc.subject or \"\" }}</div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-2\" align=\"right\"><b>Description</b></div>\n                    <div class=\"col-xs-10\">{{ doc.description or \"\"}}</div>\n                </div>\n            </div>\n        </div>\n        <hr>\n        <div class=\"row\">\n            <div class=\"col-xs-6\">\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Opening Date</b></div>\n                    <div class=\"col-xs-7\">{{ doc.opening_date or \"\"}}</div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5\" align=\"right\"><b>Opening Time</b></div>\n                    <div class=\"col-xs-7\">{{ doc.opening_time or \"\"}}</div>\n                </div>\n            </div>\n            {% if doc.due_date %}\n            <div class=\"col-xs-6\">\n                {% if doc.due_date %}\n                <div class=\"row\">\n                    <div class=\"col-xs-5\"><b>Due Date</b></div>\n                    <div class=\"col-xs-7\">{{ doc.get_formatted(\"due_date\", doc) or \"\" }}</div>\n                </div>\n                {% endif %}\n                {% if doc.due_time %}\n                <div class=\"row\">\n                    <div class=\"col-xs-5\"><b>Due Time</b></div>\n                    <div class=\"col-xs-7\">{{ doc.due_time or \"\" }}</div>\n                </div>\n                {% endif %}\n            </div>\n            {% endif %}\n        </div>\n        {% if doc.first_responded_on or doc.resolution_details or doc.resolution_date %}\n         <hr>\n        <div class=\"row\">\n            <div class=\"col-xs-6\">\n                {% if doc.first_responded_on %}\n                <div class=\"row\">\n                    <div class=\"col-xs-6\"><b>First Responded On</b></div>\n                    <div class=\"col-xs-6\">{{ doc.get_formatted(\"first_responded_on\", doc) or \"\" }}</div>\n                </div>\n                {% endif %}\n\n                <div>\n                    {% if doc.resolution_date %}\n                    <div class=\"row\">\n                        <div class=\"col-xs-6\"><b>Resolution Date</b></div>\n                        <div class=\"col-xs-6\">{{ doc.get_formatted(\"resolution_date\", doc) or \"\" }}</div>\n                    </div>\n                    {% endif %}\n                </div>\n            </div>\n            <div class=\"col-xs-6\">\n                {% if doc.resolution_details %}\n                <div class=\"row\">\n                    <div class=\"col-xs-6\"><b>Resolution Details</b></div>\n                    <div class=\"col-xs-6\">{{ doc.resolution_details or \"\" }}</div>\n                </div>\n                {% endif %}\n            </div>\n        </div>\n        {% endif %}\n        <hr>\n    </div>\n    <footer align=\"center\"><p>www.telecom-advocates.com</p></footer>\n</div>\n", 
