  "html": "{{ letter_head }}\n\n{%- if trigger_print -%}\n<script>\nwindow.print();\n\n// close the window after print\n// NOTE: doesn't close if print is cancelled in Chrome\nwindow.onfocus = function() {\nwindow.close();\n}\n</script>\n{%- endif -%}\n\n<div class=\"row\">\n    <div class=\"col-xs-6\"></div>\n    <div class=\"col-xs-6\" align=\"right\">\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <h4 style=\"margin:0;cellpadding:0\" align=\"right\"><b>SALES INVOICE</b></h4>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n<table width=60% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n              <th>Date</th>\n              <th>Invoice#</th>\n              <tr>\n                  <td>{{ doc.get_formatted(\"posting_date\",doc) or \"\" }}</td>\n                  <td>{{ doc.name or \"\" }} </td>\n              </tr>\n           </table></div>\n        </div>\n    </div>\n    <div class=\"col-xs-4\"></div>\n</div>\n<hr>\n\n<div class=\"row\">\n  <div class=\"col-xs-6\">\n      <!-- Bill To details -->\n      <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n          <th><b>Bill To</b></th>\n          <tr>\n              <td style=\"height: 130px;display: block;\">\n                  {{ doc.customer_name or \"\" }}\n              <br>{{ doc.address_display or \"\" }}\n              </td>\n          </tr>\n      </table>\n  </div>\n  <div class=\"col-xs-6\">\n      <!-- Ship To Details  -->\n      <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n          <th><b>Ship To</b></th>\n          <tr>\n              <td style=\"height: 130px;display: block;\">\n                  {{ doc.contact_display or \"\" }}\n              <br>{{ doc.shipping_address or \"\" }}\n              </td>\n          </tr>\n      </table>\n  </div>\n</div>\n<br>\n<div class=\"row\">\n  <div class=\"col-xs-12\">\n      <!-- Terms Table  -->\n      <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n          <th>Rep</th>\n          <th>PO Number</th>\n          <th>Terms</th>\n          <th>Ship Date</th>\n          <th>Via</th>\n          <th>F.O.B</th>\n          <th>Due Date</th>\n          <tr>\n              <td>{{ doc.employee or \"\"}} {% if doc.employee %} - {{ doc.employee_name or \"\"}}{% endif %}</td>\n              <td>{{ doc.po_number or \"\"}}</td>\n              <td>{{ doc.tc_name or \"\" }}</td>\n              <td>{{ doc.get_formatted(\"ship_date\") or \"\"}}</td>\n              <td>{{ doc.via or '' }}</td>\n              <td>{{ doc.fob or '' }}</td>\n              <td>{{ doc.get_formatted(\"due_date\") or \"\"}}</td>\n          </tr>\n      </table>\n  </div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-12\">\n      <!-- Item Table -->\n      <table width=100% class=table-bordered style=\"cellspacing:0;cellpadding:0;line-height=0.5;\">\n          <th>Qty</th>\n          <th>Item Code</th>\n          <th>Description</th>\n          <th>Price Each</th>\n          <th>Amount</th>\n          {%- for row in doc.items -%}\n          <tr>\n              <td align=\"right\">{{ row.qty or \"\" }}</td>\n              <td>{{ row.item_code or \"\" }}</td>\n              <td>{{ row.description or \"\" }}</td>\n              <td align=\"right\">{{ row.rate or \"\" }}</td>\n              <td align=\"right\">{{ row.amount or \"\" }}</td>\n          </tr>\n          {%- endfor -%}\n          <tr>\n              <td colspan=\"3\"><b>Payment / Credits</b></td>\n              <td colspan=\"2\" align=\"right\">\n                  {{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") or \"\" }} {{doc.total_advance or 0.0}}\n              </td>\n          </tr>\n          <tr>\n              <td colspan=\"3\"><b>Balance Due</b></td>\n              <td colspan=\"2\" align=\"right\">\n                  {{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") or \"\" }} {{ ((doc.rounded_total or 0.0) - (doc.total_advance or 0.0)) or 0.0}}\n              </td>\n          </tr>\n          <tr><td colspan=\"5\" align=\"center\"><i>Payable To - Telecom Advocates Inc</i></td></tr>\n      </table>\n  </div>\n</div>\n<footer align=\"center\">\n  <p>www.telecom-advocates.com</p>\n</footer>", 
